{
  "manifest_version": 3,
  "name": "Resumelytics ATS Helper",
  "version": "0.1.0",
  "description": "Analyze resume fit against the current job page.",
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["http://127.0.0.1:8787/*"],
  "action": {
    "default_title": "Resumelytics",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
